<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
            <stroke android:width="1dp" android:color="#34a7a9ac"/>
            <gradient android:startColor="#34a7a9ac" android:endColor="#34a7a9ac" android:angle="-90"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
            <stroke android:width="1dp" android:color="#34a7a9ac"/>
            <solid android:color="#34a7a9ac"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="20dp"/>
            <stroke android:width="1dp" android:color="#70a7a9ac"/>
            <gradient android:startColor="#70a7a9ac" android:endColor="#70a7a9ac" android:angle="-90"/>
        </shape>
    </item>
</selector>
